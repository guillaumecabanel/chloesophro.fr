<!-- Primary Meta Tags -->
<meta name="title" content="<%= data.config.metatags.title %>">
<meta name="description" content="<%= data.config.metatags.description %>">

<!-- Open Graph / Facebook -->
<meta property="og:type" content="website">
<meta property="og:url" content="<%= data.config.metatags.url %>">
<meta property="og:title" content="<%= data.config.metatags.title %>">
<meta property="og:description" content="<%= data.config.metatags.description %>">
<meta property="og:image" content="<%= data.config.metatags.url %><%= image_path "meta-image.jpg" %>">

<!-- Twitter -->
<meta property="twitter:card" content="summary_large_image">
<meta property="twitter:url" content="<%= data.config.metatags.url %>">
<meta property="twitter:title" content="<%= data.config.metatags.title %>">
<meta property="twitter:description" content="<%= data.config.metatags.description %>">
<meta property="twitter:image" content="<%= data.config.metatags.url %><%= image_path "meta-image.jpg" %>">
